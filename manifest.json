{
  "name": "Mimamori Twitter Chrome Extension",
  "description": "mimamorimasu",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://tweetdeck.twitter.com/*"],
      "js": [
        "jquery-3.1.1.min.js",
        "moment-with-locales.js",
        "mimamori-twitter-chrome-extension.js"
      ]
    }
  ],
  "options_ui":{
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
